<?xml version="1.0" encoding="utf-8"?>
<resources>
	
	<declare-styleable name="CustomTextView">
		<attr name="trimLength" format="integer" />
		<attr name="customFont" format="string" />
		<attr name="textStyle" format="integer" />
	</declare-styleable>
	<!--<declare-styleable name="Fonts">
		<attr name="font" format="string" />
		<attr name="android:textStyle" />
	</declare-styleable>-->
	
	
	<declare-styleable name="CustomShapeImageView">
		<attr name="shape" format="integer">
			<enum name="circle" value="1" />
			<enum name="rectangle" value="2" />
			<enum name="svg" value="3" />
		</attr>
		
		<attr name="svg_raw_resource" format="reference" />
	
	</declare-styleable>
	
	<declare-styleable name="MaxHeightScrollView">
		<attr name="maxHeight" format="dimension" />
	</declare-styleable>
</resources>